# Copy this file to .env and fill in your private key

# Private key of wallet to send transactions (needs ETH for gas)
# IMPORTANT: Use a separate wallet, NOT your deployment wallet!
PRIVATE_KEY=0x...your_private_key_here...

# Ethereum RPC URL (using your Infura account)
RPC_URL=https://mainnet.infura.io/v3/348bd283eeb1480ab5ce5f435995603d

# LeaguePool contract address (mainnet)
LEAGUE_POOL_ADDRESS=0x4e5e6b9c9871E2B5D42107051B174aBB13D1cdcB

# How often to check for new results (in minutes)
POLL_INTERVAL_MINUTES=5

# Etherscan API key for fetching current gas prices (get free key at etherscan.io/myapikey)
ETHERSCAN_API_KEY=YourApiKeyToken

# Twitter API credentials for automated posts (get from developer.twitter.com)
TWITTER_APP_KEY=your_app_key_here
TWITTER_APP_SECRET=your_app_secret_here
TWITTER_ACCESS_TOKEN=your_access_token_here
TWITTER_ACCESS_SECRET=your_access_secret_here
